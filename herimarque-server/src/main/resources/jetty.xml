<Configure id="Server" class="org.mortbay.jetty.Server">
	<!-- class="org.eclipse.jetty.server.Server" -->
	<Set name="handlers">
		<Array type="org.mortbay.jetty.Handler">
			<Item>
				<New class="org.mortbay.jetty.handler.ContextHandler">
					<Set name="contextPath">/aa</Set>
					<Set name="resourceBase">/data/temp/herimarqueRepo</Set>
				</New>
			</Item>

		</Array>
	</Set>
</Configure>



